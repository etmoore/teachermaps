  <h3>Resource Filter</h3>
</div>
<div class="modal-body center">
	<div class="row">
	  <div class="span9" id="map_resource_filter">
	  <%= render 'map_standards/filter_resource' %>
	  </div>
	</div>
	<div class="row">
	  <div class="span9" id="map_standards_resource_table">
	    <%= render 'map_standards/table_resources' %>
	  </div>
	</div>
</div>



