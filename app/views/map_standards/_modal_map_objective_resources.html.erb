<div class="modal-header">
  <button type="button" class="close" data-dismiss="modal">&times;</button>
  <h3><%= t('shared.my_resources')%></h3>
</div>
<div class="modal-body center">
	<div id="map_resource_filter">
	  <%= render 'map_standards/filter_map_objective_resources' %>
	</div>
	<span id="filter_map_standard_resource_loading_wheel" style="display:none;" >
  	<%= image_tag "loading.gif", :alt => "loading" %>
  </span>
  <div id="map_standards_resource_table" style="margin: 0px 20px 0px 20px;">
    <%= render 'map_standards/table_map_objective_resources' %>
  </div>
</div>
<div class="modal-footer">
	<button type="button" class="btn btn-primary" data-dismiss="modal">Save</button>
</div>
  



